                <h4 class="mb-0"><%= title %></h4>
                <small class="text-muted">Website analytics and reporting dashboard</small>
            </div>
            <div class="d-flex gap-2">
                <select class="form-select">
                    <option value="7">Last 7 days</option>
                    <option value="30">Last 30 days</option>
                    <option value="90">Last 3 months</option>
                    <option value="365">Last year</option>
                </select>
                <button class="btn btn-outline-secondary">
                    <i class="fas fa-download me-2"></i>Export Report
                </button>
            </div>
        </div>

        <!-- Key Metrics -->
        <div class="row g-4 mb-4">
            <div class="col-xl-3 col-md-6">
                <div class="metric-card">
                    <div class="metric-value">12,847</div>
                    <div class="metric-label">Total Page Views</div>
                    <div class="metric-change change-positive">
                        <i class="fas fa-arrow-up"></i> 15.3% from last month
                    </div>
                </div>
            </div>
            <div class="col-xl-3 col-md-6">
                <div class="metric-card">
                    <div class="metric-value">3,429</div>
                    <div class="metric-label">Unique Visitors</div>
                    <div class="metric-change change-positive">
                        <i class="fas fa-arrow-up"></i> 8.7% from last month
                    </div>
                </div>
            </div>
            <div class="col-xl-3 col-md-6">
                <div class="metric-card">
                    <div class="metric-value">4.2</div>
                    <div class="metric-label">Avg. Session Duration</div>
                    <div class="metric-change change-positive">
                        <i class="fas fa-arrow-up"></i> 12.1% from last month
                    </div>
                </div>
            </div>
            <div class="col-xl-3 col-md-6">
                <div class="metric-card">
                    <div class="metric-value">2.8%</div>
                    <div class="metric-label">Bounce Rate</div>
                    <div class="metric-change change-negative">
                        <i class="fas fa-arrow-down"></i> 5.2% from last month
                    </div>
                </div>
            </div>
        </div>

