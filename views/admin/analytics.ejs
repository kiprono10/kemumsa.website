                            <button class="btn btn-sm btn-outline-primary">Weekly</button>
                            <button class="btn btn-sm btn-outline-primary">Monthly</button>
                        </div>
                    </div>
                    <canvas id="trafficChart" height="300"></canvas>
                </div>
            </div>

