                            <tbody>
                                <tr>
                                    <td><strong>/</strong></td>
                                    <td>Home Page</td>
                                    <td class="text-end">
                                        <span class="badge bg-primary">3,247 views</span>
                                    </td>
                                </tr>
                                <tr>
                                    <td><strong>/events</strong></td>
                                    <td>Events</td>
                                    <td class="text-end">
                                        <span class="badge bg-success">1,892 views</span>
                                    </td>
                                </tr>
                                <tr>
                                    <td><strong>/about</strong></td>
                                    <td>About Us</td>
