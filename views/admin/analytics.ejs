            <div class="col-xl-4">
                <div class="chart-container">
                    <h5 class="mb-4">Traffic Sources</h5>
                    <canvas id="sourcesChart" height="300"></canvas>
                </div>
            </div>
        </div>

        <!-- Charts Row 2 -->
        <div class="row g-4 mb-4">
